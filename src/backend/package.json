{
  "name": "synapse-backend",
  "version": "0.1.0",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "prebuild": "node -e \"const fs = require('fs'); if (fs.existsSync('./dist')) { fs.rmSync('./dist', { recursive: true, force: true }); }\"",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "postinstall": "npm run install-chrome",
    "install-chrome": "node -e \"console.log('Installing Chrome...'); try { require('child_process').execSync('npx puppeteer browsers install chrome', {stdio: 'inherit'}); } catch(e) { console.log('Chrome installation failed, will use system Chrome'); }\""
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@hapi/boom": "^10.0.1",
    "@types/axios": "^0.9.36",
    "@types/bcryptjs": "^2.4.6",
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/form-data": "^2.2.1",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.2.5",
    "@types/node-cron": "^3.0.11",
    "@types/node-telegram-bot-api": "^0.64.8",
    "@types/qrcode": "^1.5.5",
    "@whiskeysockets/baileys": "^6.7.18",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "form-data": "^4.0.2",
    "fs-extra": "^11.3.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.8.0",
    "mongoose": "^8.14.2",
    "multer": "^2.0.0",
