# OpenAI API Key (required)
OPENAI_API_KEY=your_openai_api_key_here

# Reddit API Credentials (optional - will use simulation if not provided)
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret
REDDIT_USER_AGENT=SynapseAgent/1.0

# Telegram Bot Token (optional - will use simulation if not provided)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token

# MongoDB Connection (for storing results)
MONGODB_URI=mongodb://localhost:27017/synapse

# Service Configuration
PORT=5000
ENVIRONMENT=development